{
	"compilerOptions": {
		"outDir": "./dist/",
		"sourceMap": false,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"module": "es2020",
		"target": "es5",
		"jsx": "react",
		"allowJs": true,
		"moduleResolution": "node",
		"strict": true,
		"strictNullChecks": false,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true
	}
}
